{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOW-FROM https://www.canva.com"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors https://www.canva.com https://canva.me https://*.canva.com;"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}